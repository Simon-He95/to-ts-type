{
  "publisher": "simonhe",
  "name": "to-ts-type",
  "displayName": "To Ts Type",
  "version": "0.0.12",
  "packageManager": "pnpm@8.15.9",
  "description": "to-ts-type",
  "author": "Simon He <https://github.com/Simon-He95>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/to-ts-type#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Simon-He95/to-ts-type"
  },
  "bugs": {
    "url": "https://github.com/Simon-He95/to-ts-type/issues"
  },
  "sponsor": {
    "url": "https://github.com/Simon-He95/sponsor"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.96.0"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "to-ts-type.transform",
        "title": "to-ts-type"
      }
    ],
    "keybindings": [
      {
        "command": "to-ts-type.transform",
        "key": "cmd+alt+t",
        "when": "isMac"
      },
      {
        "command": "to-ts-type.transform",
        "key": "ctrl+alt+t",
        "when": "!isMac"
      }
    ]
  },
  "scripts": {
    "dev": "pnpm build --watch",
    "test": "vitest",
    "build": "tsup src/index.ts --external vscode",
    "pack": "vsce package --no-dependencies",
    "lint": "eslint . --cache",
    "lint:fix": "eslint . --fix",
    "publish": "vsce publish --no-dependencies",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm run publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@simon_he/eslint-config": "^0.0.6",
    "@types/node": "^18.19.68",
    "@types/vscode": "^1.96.0",
    "@vscode-use/utils": "0.1.11",
    "bumpp": "^9.9.2",
    "eslint": "^8.57.1",
    "esno": "^0.16.3",
    "flatted": "^3.3.2",
    "lazy-js-utils": "^0.1.27",
    "pnpm": "^8.15.9",
    "rimraf": "^4.4.1",
    "tsup": "^6.7.0",
    "typescript": "^5.7.2",
    "vitest": "^0.29.8",
    "vsce": "^2.15.0"
  }
}
